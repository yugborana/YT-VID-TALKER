{
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/next"
      },
      {
        "src": "api/index.py",
        "use": "@vercel/python"
      }
    ],
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/api/index.py"
      },
      {
        "source": "/(.*)",
        "destination": "/"
      }
    ]
  }